[all-bootstrap]
method                  = scp
login                   = rosbuild
fqdn                    = repos.ros.org
incoming                = /var/www/repos/ros_bootstrap/queue/all
run_dinstall            = 0
post_upload_command     = ssh rosbuild@repos.ros.org -- /usr/bin/reprepro -b /var/www/repos/ros_bootstrap --ignore=emptyfilenamepart -V processincoming all
